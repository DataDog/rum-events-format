{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "profiling/long-task-entry-schema.json",
  "title": "RumProfilerLongTaskEntry",
  "type": "object",
  "description": "Schema of a long task entry recorded during profiling.",
  "required": ["duration", "entryType", "startClocks"],
  "properties": {
    "id": {
      "type": "string",
      "description": "RUM Long Task id.",
      "readOnly": true
    },
    "duration": {
      "type": "integer",
      "description": "Duration in ns of the long task or long animation frame.",
      "minimum": 0,
      "readOnly": true
    },
    "entryType": {
      "type": "string",
      "enum": ["long-task", "long-animation-frame"],
      "description": "Type of the event: long task or long animation frame",
      "readOnly": true
    },
    "startClocks": {
      "$ref": "_clocks-state-schema.json"
    }
  }
}
