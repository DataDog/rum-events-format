{
  "name": "rum-events-format",
  "version": "1.0.0",
  "description": "JSON schema of Datadog RUM events",
  "repository": "https://github.com/DataDog/rum-events-format.git",
  "private": true,
  "license": "Apache-2.0 (See ./LICENSE)",
  "files": [
    "lib"
  ],
  "main": "lib/cjs/index.js",
  "types": "lib/cjs/index.d.ts",
  "module": "lib/esm/index.js",
  "exports": {
    "./rum": {
      "import": "./lib/esm/rum.js",
      "default": "./lib/cjs/rum.js"
    },
    "./telemetry": {
      "import": "./lib/esm/telemetry.js",
      "default": "./lib/cjs/telemetry.js"
    },
    "./session-replay": {
      "import": "./lib/esm/session-replay.js",
      "default": "./lib/cjs/session-replay.js"
    },
    "./session-replay-browser": {
      "import": "./lib/esm/session-replay-browser.js",
      "default": "./lib/cjs/session-replay-browser.js"
    },
    "./session-replay-mobile": {
      "import": "./lib/esm/session-replay-mobile.js",
      "default": "./lib/cjs/session-replay-mobile.js"
    }
  },
  "typesVersions": {
    ">=4.5": {
      "rum": [
        "./lib/esm/rum.d.ts"
      ],
      "telemetry": [
        "./lib/esm/telemetry.d.ts"
      ],
      "session-replay": [
        "./lib/esm/session-replay.d.ts"
      ],
      "session-replay-browser": [
        "./lib/esm/session-replay-browser.d.ts"
      ],
      "session-replay-mobile": [
        "./lib/esm/session-replay-mobile.d.ts"
      ]
    }
  },
  "type": "module",
  "scripts": {
    "build": "yarn generate && rm -rf ./lib && yarn build:cjs && yarn build:esm",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "generate": "node scripts/generate.js",
    "format": "prettier -c ."
  },
  "devDependencies": {
    "json-schema-to-typescript": "bcaudan/json-schema-to-typescript#bcaudan/add-readonly-support",
    "prettier": "2.7.1",
    "typescript": ">=4.5"
  },
  "engines": {
    "node": ">=12.7.0"
  },
  "volta": {
    "node": "16.3.0",
    "yarn": "1.22.18"
  }
}
